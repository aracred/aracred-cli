{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": [],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "semi-style": "off",
    "linebreak-style": [2, "unix"],
    "semi": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false
      }
    ]
  }
}
